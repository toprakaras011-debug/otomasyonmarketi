{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:critical": "playwright test e2e/critical",
    "test:e2e:fast": "playwright test --project=chromium",
    "test:e2e:critical:fast": "playwright test e2e/critical --project=chromium",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e:critical",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build",
    "performance:audit": "lighthouse http://localhost:3000 --output=html --output-path=./performance-audit.html",
    "security:audit": "npm audit --audit-level=moderate",
    "optimize": "npm run analyze && npm run performance:audit && npm run security:audit",
    "vercel-build": "next build"
  },
